    #Include "TextLib" as TL

    main(){
        wait(InputPlayer != Null);
        declare Text TL_Hash for InputPlayer;
        declare Integer TL_Limit for InputPlayer;

        TL_Hash = {uniqid()};
        TL_Limit = {$timeLimit};
    }

    --></script>
