<manialink version="3" id="update-custom-names" name="EvoSC:update-custom-names">
    <script><!--
    main(){
        declare Text[Text] EvoSC_CustomNames for LocalUser;
        declare Text[Text] EvoSC_CustomNamesByUbiname for LocalUser;

        declare trans = """{$keyedByLogin->toJson()}""";
        EvoSC_CustomNames.fromjson(trans);
        trans = """{$keyedByUbiname->toJson()}""";
        EvoSC_CustomNamesByUbiname.fromjson(trans);
    }
    --></script>
