    "composer/ca-bundle": "^1.1",
    "manialib/gbx": "^4.1",
    "larapack/dd": "^1.1",
    "illuminate/container": "^8.27",
    "illuminate/database": "^8.27",
    "mexitek/phpcolors": "^1.0",
    "league/csv": "^9.7"
  },
  "scripts": {
    "pre-install-cmd": [
      "php core/required_directories.php"
    ]
